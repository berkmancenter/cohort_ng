<tr>	
  <td>
    <span style="font-size: .70em; margin-left: 5px;">  
	  <%= link_to(icon('pencil'), edit_user_path(user), :title => 'Edit User', :class => 'dialog-form') %>
	  <% unless user == current_user %>
        <%= link_to(icon('cross'), user_path(user), :title => 'Delete User', :method => :delete, :class => 'delete delete-user') %>
      <% end %>	
    </span>	
  </td>	
  <td><%= link_to(user.email, user ) %></td>
</tr>

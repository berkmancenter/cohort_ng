<li class="contact-cart-<%= contact_cart.id %>">
<% unless current_user.blank? %>
  <% if params[:select_for_contact_cart].blank? %>
    <a href="#" class="control" id="contact_cart-control-<%= contact_cart.id %>"><%= icon('cog') %></a>
    <%= link_to(contact_cart, contact_cart, :class => 'dialog-show') %>
  <% else %>
    <a href="#" class="select_for_contact_cart" data_contact_cart_id="<%= contact_cart.id %>"><%= icon('basket_add') %></a>
    <%= link_to(contact_cart, contact_cart, :class => 'select_for_contact_cart', :data_contact_cart_id => contact_cart.id) %>
  <% end %>
<% end %>
<% unless current_user.blank? %>
  <% if params[:select_for_contact_cart].blank? %>
    <span id="contact_cart-control-<%= contact_cart.id %>-target" style="display: none;"><%= link_to(icon('note_edit') + ' Edit', edit_contact_cart_path(contact_cart),:class => 'dialog-form', :title => 'Edit contact list') %><br/><%= link_to(icon('note_delete') + ' Delete',contact_cart_path(contact_cart), :method => :delete, :title => 'Delete contact list', :class => 'delete delete-contact_cart') %></span>
  <% end %>
<% end %>
</li>

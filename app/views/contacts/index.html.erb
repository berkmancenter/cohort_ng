<div class="grid_5 alpha">
  <h2>Contacts</h2>
  <h4><%= link_to('Add New', new_contact_path, :class => 'dialog-form', :title => "New Contact") %></h4>
  <div class="tabs">
    <ul>
      <% unless current_user.blank? %>
        <li><%= link_to('Mine', contact_query_yours_path) %></li>
      <% end %>
	  <li><%= link_to('All', contact_query_all_path) %></li>
      <li><%= link_to('New', contact_query_new_path) %></li>
      <li><%= link_to('Updated', contact_query_recent_path) %></li>
    </ul>

  </div>
</div>

<div class="grid_5 alpha">
  <h2>Contact Lists</h2>
  <h4><%= link_to 'Add New', new_contact_cart_path, :class => 'dialog-form', :title => 'New Contact List' %></h4>
  <div class="tabs">
    <ul>
      <% unless current_user.blank? %>
        <li><%= link_to 'Mine', contact_cart_query_yours_path %></li>
      <% end %>
	  <li><%= link_to('All', contact_cart_query_all_path) %></li>
    </ul>

  </div>
</div>
